    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.28.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mocha": "^10.7.3",
    "npm-run-all2": "^6.2.3",
