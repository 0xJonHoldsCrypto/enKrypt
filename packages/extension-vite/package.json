    "@solana/spl-token": "^0.4.8",
    "@solana/wallet-standard-features": "^1.2.0",
    "@solana/web3.js": "^1.95.3",
    "@wallet-standard/base": "^0.0.0-20240703212708",
    "add": "^2.0.6",
    "bignumber.js": "^9.1.2",
