{
  "name": "@enkryptcom/extension",
  "version": "0.3.1",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build && npm run build:rollup",
    "lint": "vue-cli-service lint --fix",
    "build:rollup": "cross-env minify=on rollup --config configs/rollup.config.contentscript.js && cross-env minify=on rollup --config configs/rollup.config.inject.js",
    "inspectWebpack": "vue-cli-service inspect > webpack.log",
    "test": "ts-mocha --paths -p configs/tsconfig.test.json ./**/*.mocha.ts",
    "watch": "concurrently 'npm:watch-*'",
    "watch-contentscript": "rollup --watch --config configs/rollup.config.contentscript.js",
    "watch-inject": "rollup --watch --config configs/rollup.config.inject.js",
    "watch-vue": "vue-cli-service build --watch --no-clean"
  },
  "dependencies": {
    "@enkryptcom/extension-bridge": "^0.0.1",
    "@enkryptcom/keyring": "^0.0.1",
    "@enkryptcom/request": "^0.0.1",
    "@enkryptcom/storage": "^0.0.1",
    "@enkryptcom/utils": "^0.0.1",
    "@polkadot/types": "^7.10.1",
    "@polkadot/util": "^8.4.1",
    "@types/chrome": "^0.0.180",
    "@types/events": "^3.0.0",
    "chai": "^4.3.6",
    "concurrently": "^7.0.0",
    "core-js": "^3.21.0",
    "ethereumjs-util": "^7.1.4",
    "eventemitter3": "^4.0.7",
    "moment": "^2.29.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "url-parse": "^1.5.10",
    "vue": "^3.2.31",
    "vue-router": "4",
    "vue3-lottie": "^2.1.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@polkadot/api": "^7.10.1",
    "@polkadot/extension-inject": "^0.42.7",
    "@polkadot/rpc-provider": "^7.10.1",
    "@polkadot/types": "^7.10.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/mocha": "^9.1.0",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "babel-code-frame": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.1.2",
    "less-loader": "^5.0.0",
    "mocha": "^9.2.0",
    "node-ipc": "^11.1.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.4.1",
    "rollup": "^2.68.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-mocha": "^9.0.2",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "webextension-polyfill": "^0.8.0",
    "worker-rpc": "^0.2.0"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not safari > 0",
    "not ios_saf > 0",
    "not baidu > 0",
    "not ie > 0",
    "not op_mini all",
    "not samsung > 0",
    "not op_mob > 0",
    "not android > 0",
    "not kaios > 0",
    "not and_chr > 0",
    "not and_ff > 0",
    "not and_qq > 0",
    "not and_uc > 0"
  ],
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
